Xft.dpi: 96
Xft.lcdfilter: lcddefault
Xft.antialias: true
Xft.autohint: false
Xft.rgba: rgb
Xft.hinting: true
Xft.hintstyle: hintfull

*.font: xft:Ubuntu Mono:size=11

URxvt.perl-ext-common: default,clipboard
URxvt.keysym.Shift-Control-C: perl:clipboard:copy
URxvt.keysym.Shift-Control-V: perl:clipboard:paste
URxvt.iso14755: False

URxvt.scrollBar: false
URxvt*depth: 32

URxvt.background: #2d2d2d
URxvt.foreground: #dedede
URxvt.cursorColor: #dedede

! black
URxvt.color0:  #2d2d2d
URxvt.color8:  #dedede

! red
URxvt.color1:  #d64937
URxvt.color9:  #d64937

! green
URxvt.color2:  #61c437
URxvt.color10: #61c437

! yellow
URxvt.color3:  #e9a50d
URxvt.color11: #f5c14e

! blue
URxvt.color4:  #2789BF
URxvt.color12: #31ACEF

! magenta
URxvt.color5:  #bd3075
URxvt.color13: #bd3075

! cyan
URxvt.color6:  #00b7b7
URxvt.color14: #00b7b7

! white
URxvt.color7:  #dedede
URxvt.color15: #dedede
